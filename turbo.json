{
    "$schema": "https://turborepo.com/schema.json",
    "ui": "tui",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "inputs": [
                "$TURBO_DEFAULT$",
                ".env*"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "lint": {
            "dependsOn": [
                "^lint"
            ]
        },
        "check-types": {
            "dependsOn": [
                "^check-types"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "@radroots/core#build": {
            "dependsOn": [
                "@radroots/tsconfig#build"
            ]
        },
        "@radroots/events#build": {
            "dependsOn": [
                "@radroots/tsconfig#build",
                "@radroots/core#build"
            ]
        },
        "@radroots/events-indexed#build": {
            "dependsOn": [
                "@radroots/tsconfig#build"
            ]
        },
        "@radroots/trade#build": {
            "dependsOn": [
                "@radroots/tsconfig#build",
                "@radroots/core#build",
                "@radroots/events#build"
            ]
        }
    }
}
